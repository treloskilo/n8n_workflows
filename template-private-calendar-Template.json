    {
      "parameters": {
        "toolDescription": "Use this tool to find email addresses when only a person's first name or full name is provided. Input:\n- name: The person's full or partial name to search\nReturns: Contact details including email address. Use this before scheduling meetings when email addresses aren't provided.",
        "method": "POST",
        "url": "https://api.attio.com/v2/objects/people/records/query",
        "authentication": "genericCredentialType",
        "genericAuthType": "httpHeaderAuth",
        "sendBody": true,
        "specifyBody": "json",
        "jsonBody": "{\n  \"filter\": {\n    \"$or\": [\n      {\n        \"name\": {\n          \"$contains\": \"{name}\"\n        }\n      },\n      {\n        \"email_addresses\": {\n          \"$contains\": \"{name}\"\n        }\n      }\n    ]\n  }\n}\n",
        "placeholderDefinitions": {
          "values": [
            {
              "name": "name",
              "description": "The name of the person to search for",
              "type": "string"
            }
          ]
        }
      },
